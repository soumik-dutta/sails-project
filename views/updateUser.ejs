<form action='/User/update/<%= things.id %>' method='post'>
   Name:    <input type="text" name="name" data-ng-model='name' value="<%= things.name %>" ><br>
   College: <input type="text" name="college" data-ng-model='college' value="<%= things.college %>"><br/>
   Stream:  <input type="text" name="stream" data-ng-model='stream' value="<%= things.stream %>"><br/>
       <input type="submit" value="update">
</form>
