<div ng-app="student" ng-controller="BaseCtrl">
  <h1>Student Database</h1>
    <table>
        <tr>
          <th>Name</th>
          <th>College</th>
          <th>Straem</th>
        </tr>
        <tr ng-repeat="thing in things">
          <td>{{thing.name}}</td>        
          <td>{{thing.college}}</td>        
          <td>{{thing.stream}}</td>  
        </tr>
    </table>
</div>
